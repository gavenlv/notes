{
  "dashboard": {
    "id": null,
    "title": "Apache Airflow Monitoring",
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "DAG Runs Status Over Time",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(airflow_dag_run_duration_seconds_count{state=\"success\"}[1h]))",
            "legendFormat": "Successful Runs",
            "refId": "A"
          },
          {
            "expr": "sum(increase(airflow_dag_run_duration_seconds_count{state=\"failed\"}[1h]))",
            "legendFormat": "Failed Runs",
            "refId": "B"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Count"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Active DAG Runs",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "airflow_dag_run_current_active",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto"
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Task Failures (Last 24h)",
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(increase(airflow_task_failures_total[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto"
        }
      },
      {
        "id": 4,
        "type": "graph",
        "title": "Worker Queue Size",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "airflow_worker_queue_size",
            "legendFormat": "Queue Size",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Tasks"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 5,
        "type": "table",
        "title": "Top 10 Longest Running DAGs",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "topk(10, airflow_dag_run_duration_seconds_sum / airflow_dag_run_duration_seconds_count)",
            "legendFormat": "{{dag_id}}",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "seriesToRows",
            "options": {}
          }
        ],
        "options": {
          "showHeader": true
        }
      },
      {
        "id": 6,
        "type": "graph",
        "title": "Database Connections",
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "pg_stat_activity_count{datname=\"airflow\"}",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Connections"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 7,
        "type": "graph",
        "title": "Redis Memory Usage",
        "gridPos": {
          "x": 12,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "redis_memory_used_bytes",
            "legendFormat": "Memory Used",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "bytes",
            "label": "Memory"
          },
          {
            "format": "short"
          }
        ]
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "links": [],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ]
    }
  }
}