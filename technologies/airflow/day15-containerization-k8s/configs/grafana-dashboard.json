{
  "dashboard": {
    "id": null,
    "title": "Airflow Kubernetes Monitoring",
    "tags": ["airflow", "kubernetes"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "Airflow Task Duration",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(airflow_task_duration_seconds_bucket[5m])) by (le, dag_id))",
            "legendFormat": "{{dag_id}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Running Tasks",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "airflow_tasks_running",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "calcs": ["last"]
          },
          "orientation": "horizontal",
          "textMode": "auto"
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Failed Tasks",
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "increase(airflow_tasks_failed_total[1h])",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "calcs": ["last"]
          },
          "orientation": "horizontal",
          "textMode": "auto"
        }
      },
      {
        "id": 4,
        "type": "graph",
        "title": "Scheduler Heartbeat",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "airflow_scheduler_heartbeat",
            "legendFormat": "Heartbeat",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "none",
            "label": "Heartbeat"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 5,
        "type": "table",
        "title": "DAG Status",
        "gridPos": {
          "x": 12,
          "y": 4,
          "w": 12,
          "h": 12
        },
        "targets": [
          {
            "expr": "airflow_dag_status",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              }
            }
          }
        ],
        "options": {
          "showHeader": true
        }
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "links": []
  }
}