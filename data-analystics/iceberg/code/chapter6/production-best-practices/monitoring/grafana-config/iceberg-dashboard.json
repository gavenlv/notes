{
  "dashboard": {
    "id": null,
    "title": "Iceberg Production Monitoring",
    "tags": ["iceberg", "apache", "big-data"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "Table Metadata Refresh Duration",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "iceberg_table_metadata_refresh_duration_seconds",
            "legendFormat": "{{table}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Duration (seconds)"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "type": "graph",
        "title": "Snapshot Creation Rate",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(iceberg_snapshot_creation_total[5m])",
            "legendFormat": "{{table}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "ops",
            "label": "Snapshots per second"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Active Tables",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "count(count by (table) (iceberg_table_metadata_refresh_duration_seconds))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Total File Operations",
        "gridPos": {
          "x": 6,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(iceberg_file_operation_total)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 5,
        "type": "graph",
        "title": "File Operation Latency",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "iceberg_file_operation_duration_seconds",
            "legendFormat": "{{operation}} - {{table}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Duration (seconds)"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 6,
        "type": "graph",
        "title": "Query Performance",
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "iceberg_query_duration_seconds",
            "legendFormat": "{{table}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Duration (seconds)"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 7,
        "type": "heatmap",
        "title": "File Size Distribution",
        "gridPos": {
          "x": 0,
          "y": 20,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "iceberg_file_size_bytes_bucket",
            "format": "heatmap",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "xAxis": {
          "show": true
        },
        "yAxis": {
          "format": "bytes",
          "logBase": 1
        },
        "color": {
          "mode": "opacity"
        }
      },
      {
        "id": 8,
        "type": "graph",
        "title": "Partition Count by Table",
        "gridPos": {
          "x": 12,
          "y": 20,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "iceberg_partition_count",
            "legendFormat": "{{table}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Partition Count"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 9,
        "type": "alertlist",
        "title": "Alerts",
        "gridPos": {
          "x": 0,
          "y": 28,
          "w": 24,
          "h": 6
        },
        "show": "current",
        "sortOrder": 1,
        "stateFilter": ["alerting", "pending", "no_data"]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "table",
          "type": "query",
          "datasource": "Prometheus",
          "refresh": 1,
          "options": [],
          "includeAll": true,
          "allValue": ".*",
          "query": "label_values(iceberg_table_metadata_refresh_duration_seconds, table)",
          "hide": 0,
          "label": "Table"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Annotations & Alerts",
          "enable": true,
          "datasource": "Prometheus",
          "showIn": 0,
          "type": "dashboard",
          "builtIn": 1,
          "hide": true
        }
      ]
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ]
    }
  }
}